


module ls(input bit osc, output  bit clk_p, clk_n, red_p, red_n, green_p, green_n, blue_p, blue_n);


logic[7:0] red = 8'h00, 
           green = 8'h00, 
			  blue = 8'h00;

wire[10:0] w_x, w_y;
			  

	hdmi hdmi_inst(.osc(osc), .red(red), .green(green), .blue(blue), .clk_p(clk_p), .clk_n(clk_n), 
	                          .red_p(red_p), .red_n(red_n), .green_p(green_p), .green_n(green_n), 
									  .blue_p(blue_p), .blue_n(blue_n), .x(w_x), .y(w_y)); 

	
	always @(posedge clk_p) begin
		
		blue <= (w_x >= 100 && w_x < 900) && (w_y >= 100 && w_y < 200) ? 8'hff : 0;
	
	end
	
	
endmodule: ls
